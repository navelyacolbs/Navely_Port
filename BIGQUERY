--BigQuery Examples
SELECT FIRST_NAME
FROM CUSTOMER_DATA.NAME
WHERE FIRST_NAME = 'NAVELY'

--SELECTING MULTIPLE COLUMNS FROM THE MAIN EXAMPLE
SELECT FIRST_NAME, LAST_NAME, IDENTIFICATION
FROM CUSTOMER_DATA.NAME
WHERE FIRST_NAME = 'NAVELY'

--SELECTING MULTIPLE COLUMNS FROM THE MAIN EXAMPLE AND ADDING A WHERE CLAUSE WITH AN OPERATOR
SELECT FIRST_NAME, LAST_NAME, IDENTIFICATION
FROM CUSTOMER_DATA.NAME
WHERE IDENTIFICATION < 4
  AND FIRST_NAME = 'NAVELY'
  AND LAST_NAME = 'ALBA_COLBERT'
